APP_NAME=Laravel
APP_ENV=production
APP_KEY=   # leave blank; Railway will use its own or generate with `php artisan key:generate`
APP_DEBUG=false
APP_URL=https://microsite-dashboard-production.up.railway.app

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=sqlite

SESSION_DRIVER=file
SESSION_LIFETIME=120

QUEUE_CONNECTION=sync
CACHE_STORE=file

MAIL_MAILER=log
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

VITE_APP_NAME="${APP_NAME}"